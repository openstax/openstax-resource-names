{
  "name": "@project/lambdas",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "coverage-report": "open coverage/index.html",
    "clean": "rm -rf dist build",
    "start": "./script/build.bash --watch",
    "data:seed": "cp -n ./data/seeds/* ./data/ || true",
    "data:clean": "find data/ -maxdepth 1 -type f -delete",
    "data:reset": "yarn data:clean && yarn data:seed",
    "build": "./script/build.bash",
    "build:clean": "yarn clean && yarn build",
    "archive:bundle": "yarn webpack --mode production",
    "archive:zip": "./script/bundle-functions.bash",
    "archive": "yarn archive:bundle && yarn archive:zip",
    "archive:clean": "yarn clean && yarn archive",
    "test": "yarn jest",
    "ci:test": "yarn jest --coverage",
    "ci:typecheck": "tsc --noEmit",
    "ci:spelling": "git ls-files | yarn cspell -c ../../cspell.json --file-list stdin",
    "ci:lint": "eslint --max-warnings=0 .",
    "ci": "CI=true npm-run-all ci:*"
  },
  "dependencies": {
    "@openstax/open-search-client": "^0.1.0-build.7",
    "@openstax/orn-locator": "^1.1.22",
    "@openstax/ts-utils": "1.2.0",
    "aws-sdk": "^2.1145.0",
    "body-parser": "^1.19.2",
    "node-fetch": "^2",
    "path-to-regexp": "^6.2.0",
    "query-string": "^7.1.1",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.92",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.35",
    "@types/node-fetch": "^2.6.1",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "builtin-modules": "^3.2.0",
    "cspell": "^5.18.5",
    "eslint": "^8.10.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "jest": "^29.1.1",
    "nock": "^13.2.9",
    "npm-run-all": "^4.1.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.3.0",
    "typescript": "4.6",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "yargs": "^17.5.1"
  }
}
